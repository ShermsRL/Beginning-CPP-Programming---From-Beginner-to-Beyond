// Section 5
// Basic I/O using cin and cout

#include <iostream>

using namespace std; // This is similar to python import all

/* Alternatively, you can jus "import" what you need
 using std::cout;
 using std::cin; 
 etc etc*/

int main() {
//    cout << "Hello world!" << endl;
//    cout << "Hello " << "World!" << endl;
//    cout << "Hello " << "World!\n";

    int num1;
    int num2;
    double num3;
    
    cout << "Enter a first integer: ";
    cin >> num1;
    cout << "Enter a second integer: ";
    cin >> num2;
    cout << "You entered " << num1 << " and " << num2 << endl;
    
    return 0;
    
}
